{
  "name": "Callr",
  "description": "Transribe and Summarize Google Meet meetings",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "downloads",
    "tabs"
  ],
  "content_scripts": [
  	{
  		"matches": ["https://meet.google.com/*"],
      "js": ["jquery-3.6.0.min.js", "jquery-ui.min.js", "script.js", "contentScript.js"],
      "css": ["style.css", "jquery-ui.min.css", "bootstrap.min.css"]
  	}
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {},
  "web_accessible_resources": [
    {
      "resources": ["style.css", "jquery-ui.min.css", "bootstrap.min.css"],
      "matches": [ "https://meet.google.com/*" ]
    }
  ]
}